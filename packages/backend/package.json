{
  "name": "@danci/backend",
  "version": "1.0.0",
  "description": "Backend service for vocabulary learning application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:win": "scripts\\dev.cmd",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down -v",
    "test:ci": "npm run test:db:up && npm run test && npm run test:db:down",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "fix:next-review-date": "tsx scripts/fix-next-review-date.ts",
    "fix:next-review-date:execute": "tsx scripts/fix-next-review-date.ts --execute",
    "fix:next-review-date:verify": "tsx scripts/fix-next-review-date.ts --verify"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@danci/shared": "workspace:*",
    "@paralleldrive/cuid2": "^3.0.4",
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^10.29.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "pino": "^10.1.0",
    "pino-abstract-transport": "^3.0.0",
    "pino-http": "^11.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^6.0.3",
    "ioredis-mock": "^8.13.1",
    "pino-pretty": "^13.1.2",
    "prisma": "^5.7.0",
    "seedrandom": "^3.0.5",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0"
  }
}
