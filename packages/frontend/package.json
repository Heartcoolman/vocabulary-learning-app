{
  "name": "@danci/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.less",
    "src/main.tsx",
    "src/config/sentry.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/components && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/pages && pnpm -s test:hooks && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/contexts && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/services && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/stores && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/utils",
    "test:watch": "vitest --project unit",
    "test:components": "vitest run --project unit src/components",
    "test:pages": "vitest run --project unit src/pages",
    "test:services": "vitest run --project unit src/services",
    "test:hooks:core": "vitest run --project unit src/hooks/__tests__",
    "test:hooks:mutations": "vitest run --project unit src/hooks/mutations",
    "test:hooks:queries": "vitest run --project unit src/hooks/queries",
    "test:hooks": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/hooks/__tests__ && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/hooks/mutations && NODE_OPTIONS='--max-old-space-size=8192' vitest run --project unit src/hooks/queries",
    "test:contexts": "vitest run --project unit src/contexts",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=6144' vitest run --project unit --coverage",
    "lint": "eslint src",
    "clean": "rm -rf dist node_modules",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@danci/shared": "workspace:*",
    "@mediapipe/tasks-vision": "0.10.22-rc.20250304",
    "@phosphor-icons/react": "^2.1.10",
    "@sentry/react": "^10.29.0",
    "@tanstack/react-query": "^5.90.12",
    "@types/react-window": "^2.0.0",
    "framer-motion": "^12.23.24",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-window": "^2.2.3",
    "uuid": "^13.0.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-onboarding": "^10.2.1",
    "@storybook/addon-vitest": "^10.2.1",
    "@storybook/react-vite": "^10.2.1",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^10.1.4",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.3",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^10.2.1",
    "tailwindcss": "^3.3.6",
    "typescript-eslint": "^8.48.1",
    "vite": "^5.0.8"
  }
}
